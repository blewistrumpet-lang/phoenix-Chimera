  "parameter_semantics": {
    "warmth": {
      "engines": [15, 16, 7, 8],
      "adjustments": {
        "ENGINE_TUBE_SATURATION": {
          "param0": {"target": 0.4, "reason": "moderate drive for warmth"},
          "param1": {"target": 0.7, "reason": "asymmetric for even harmonics"}
        },
        "ENGINE_TAPE_SATURATION": {
          "param0": {"target": 0.6, "reason": "healthy input level"},
          "param1": {"target": 0.4, "reason": "gentle compression"}
        },
        "ENGINE_PARAMETRIC_EQ": {
          "param4": {"target": 0.6, "reason": "boost 200-500Hz"},
          "param10": {"target": 0.3, "reason": "gentle high cut"}
        }
      }
    },
    "brightness": {
      "engines": [7, 21],
      "adjustments": {
        "ENGINE_PARAMETRIC_EQ": {
          "param9": {"target": 0.7, "reason": "high shelf boost"},
          "param10": {"target": 0.7, "reason": "8-12kHz air"}
        },
        "ENGINE_HARMONIC_EXCITER_PLATINUM": {
          "param3": {"target": 0.8, "reason": "focus on highs"},
          "param1": {"target": 0.4, "reason": "moderate excitement"}
        }
      }
    },
    "punch": {
      "engines": [5, 1],
      "adjustments": {
        "ENGINE_TRANSIENT_SHAPER": {
          "param0": {"target": 0.7, "reason": "enhance attack"},
          "param1": {"target": 0.3, "reason": "reduce sustain"}
        },
        "ENGINE_CLASSIC_COMPRESSOR": {
          "param2": {"target": 0.2, "reason": "fast attack"},
          "param1": {"target": 0.6, "reason": "4:1 ratio"}
        }
      }
    },
    "space": {
      "engines": [41, 42, 31, 30],
      "adjustments": {
        "ENGINE_HALL_REVERB": {
          "param0": {"target": 0.8, "reason": "large space"},
          "param1": {"target": 0.6, "reason": "3-4s decay"}
        },
        "ENGINE_DIMENSION_EXPANDER": {
          "param0": {"target": 0.7, "reason": "expanded size"},
          "param1": {"target": 0.5, "reason": "moderate amount"}
        }
      }
    },
    "dirt": {
      "engines": [17, 18, 19],
      "adjustments": {
        "ENGINE_TRANSISTOR_FUZZ": {
          "param0": {"target": 0.7, "reason": "heavy fuzz"},
          "param2": {"target": 0.3, "reason": "darker tone"}
        },
        "ENGINE_BIT_CRUSHER": {
          "param0": {"target": 0.6, "reason": "8-bit character"},
          "param1": {"target": 0.5, "reason": "moderate downsample"}
        }
      }
    },
    "width": {
      "engines": [54, 23, 30, 38],
      "adjustments": {
        "ENGINE_STEREO_IMAGER": {
          "param0": {"target": 0.8, "reason": "expanded width"},
          "param1": {"target": 0.7, "reason": "mono bass for clarity"}
        },
        "ENGINE_DIMENSION_EXPANDER": {
          "param0": {"target": 0.6, "reason": "spatial expansion"},
          "param2": {"target": 0.7, "reason": "bright dimension"}
        }
      }
    },
    "vintage": {
      "engines": [2, 16, 33, 8],
      "preferred_order": ["compression", "saturation", "delay", "reverb"],
      "adjustments": {
        "ENGINE_TAPE_SATURATION": {
          "param2": {"target": 0.3, "reason": "subtle wow/flutter"},
          "param3": {"target": 0.4, "reason": "vintage HF loss"}
        },
        "ENGINE_VINTAGE_EQ": {
          "param4": {"target": 0.5, "reason": "tube saturation"},
          "param0": {"target": 0.6, "reason": "warm low boost"}
        }
      }
    },
    "modern": {
      "engines": [31, 3, 6, 45],
      "preferred_order": ["eq", "compression", "delay", "reverb"],
      "adjustments": {
        "ENGINE_DIGITAL_DELAY": {
          "param3": {"target": 0.9, "reason": "pristine highs"},
          "param0": {"target": 0.5, "reason": "precise timing"}
        },
        "ENGINE_MASTERING_LIMITER": {
          "param3": {"target": 0.5, "reason": "lookahead enabled"},
          "param1": {"target": 0.2, "reason": "-3dB ceiling"}
        }
      }
    }
  },
  "signal_flow_rules": {
    "standard_chains": {
      "vocal": ["gate", "compression", "eq", "reverb"],
      "drums": ["transient", "compression", "eq", "reverb"],
      "bass": ["compression", "eq", "saturation"],
      "guitar": ["gate", "overdrive", "eq", "delay", "reverb"],
      "synth": ["filter", "distortion", "modulation", "delay", "reverb"]
    },
    "effect_order_priority": {
      "dynamics": 1,
      "pitch": 2,
      "eq": 3,
      "filters": 4,
      "distortion": 5,
      "modulation": 6,
      "delay": 7,
      "reverb": 8,
      "spatial": 9
    },
    "never_after": {
      "reverb": ["distortion", "compression"],
      "delay": ["pitch_shift"],
      "compression": ["reverb", "delay"]
    }
  },
  "mix_balance_rules": {
    "default_wet_dry": {
      "dynamics": 1.0,
      "eq": 1.0,
      "filters": 0.7,
      "distortion": 0.8,
      "modulation": 0.5,
      "delay": 0.35,
      "reverb": 0.3,
      "pitch": 0.5,
      "spatial": 0.6
    },
    "cumulative_wetness": {
      "max_total": 0.7,
      "reduction_per_effect": 0.05
    }
  },
  "genre_patterns": {
    "electronic": {
