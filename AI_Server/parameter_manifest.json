      "rule": "prevent_distortion_overload",
      "description": "Limit combined drive/distortion across all slots",
      "condition": "sum(drive_params) > 3.0",
      "action": "scale_down"
    },
    {
      "rule": "maintain_headroom",
      "description": "Ensure output doesn't clip",
      "condition": "estimated_peak > 0.95",
      "action": "reduce_output"
    }
  ]
